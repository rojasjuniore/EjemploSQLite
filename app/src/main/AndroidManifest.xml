<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="rojasjuniore.com.ejemplosqlite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Activities.Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Activities.Activities.InsertActivity" />
        <activity android:name=".Activities.Activities.SearchActivity" />
        <activity android:name=".Activities.Activities.DeleteActivity" />
        <activity android:name=".Activities.Activities.UpdateActivity"></activity>
    </application>

</manifest>